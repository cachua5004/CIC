@{
    ViewBag.Title = "Events";
}

<div class="full_banner">
    <img src="~/Images/img/events/banner.jpg" alt="" />
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <div class="single_caption">
                    <h1>Events Calendar</h1>
                </div>
                <!--end single caption-->
            </div>
        </div>
    </div>
</div>
<!--end full banner-->

<div class="calendar_outer">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <div class="event_calendar">
                    <div id='calendar'></div>
                </div>
                <!--end event_calendar-->
            </div>
            <!--end col-xs-12-->
        </div>
        <!--end row-->
    </div>
    <!--end container-->
</div>
<!--end calendar outer-->
